//水仙花数指一个三位数每个位数数字立方和等于该数本身
#include <stdio.h>
#include <stdlib.h>
#include "math.h"

int main()
{

    int a,b,c;


    for(int i=100; i<=999; i++)
        {a=i/100;
        b=(i-a*100)/10;
        c=i-a*100-b*10;
        if(a*a*a+b*b*b+c*c*c == i)
            {printf("%d是水仙花数\n",i);}
        }
        getchar();


    return 0;
}
